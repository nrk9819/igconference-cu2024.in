---
import { TOPBAR_LEFT,TOPBAR_RIGHT } from "../../consts";

interface Props {
    id: string
}

const {id} = Astro.props
---

<div id={id} class="py-3 px-4 md:py-4 bg-slate-800 text-white">
    <div class="container mx-auto flex justify-between text-sm md:text-base">
        <p>{TOPBAR_LEFT}</p>
        <p>{TOPBAR_RIGHT}</p>
    </div>
  </div>